<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.river">
	<namespace>Sample</namespace>
    <entity name="rivers" local-service="true" remote-service="false">
        <column name="riverid" type="int" primary="true" id-type="increment"></column>
