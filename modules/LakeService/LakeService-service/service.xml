<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.lake">
	<namespace>Sample</namespace>
    <entity name="lakes" local-service="true" remote-service="false" table="sample_lakes">
        <column name="lakeid" type="int" primary="true" id-type="increment"></column>
        <column name="name" type="String"></column>
        <column name="url" type="String"></column>
        <column name="jan" type="double"></column>
        <column name="feb" type="double"></column>
        <column name="mar" type="double"></column>
        <column name="apr" type="double"></column>
        <column name="may" type="double"></column>
        <column name="june" type="double"></column>
        <column name="july" type="double"></column>
        <column name="aug" type="double"></column>
        <column name="sep" type="double"></column>
        <column name="oct" type="double"></column>
        <column name="nov" type="double"></column>
        <column name="dec_" type="double"></column>
    </entity>
	
</service-builder>